{
    "expo": {
      "name": "EPC",
      "slug": "epc",
      "version": "1.0.6",
      "orientation": "portrait",
      "icon": "./exterior_assets/icon.png",
      "splash": {
        "image": "./exterior_assets/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#0c1e34"
      },
      "scheme": "epc",
      "updates": {
        "fallbackToCacheTimeout": 0
      },
      "assetBundlePatterns": [
        "**/*"
      ],
      "packagerOpts": {
          "sourceExts": [
            "js",
            "json",
            "ts",
            "tsx",
            "jsx",
            "vue"
          ]
        },
      "ios": {
        "bundleIdentifier": "com.epc.epcapp",
        "supportsTablet": true,
        "buildNumber": "1.0.6",
        "infoPlist": {
            "NSLocationAlwaysUsageDescription": "Find chefs or events in your area.",
            "NSLocationWhenInUseUsageDescription": "Find chefs or events in your area."
        },
        "config": {
            "googleSignIn": {
                "reservedClientId": "com.googleusercontent.apps.37474980291-3lslvcsjoddq21tq150pr96v313g9tfe"
            }
        },
        "googleServicesFile": "./GoogleService-Info.plist"
      },
      "android": {
        "adaptiveIcon": {
          "foregroundImage": "./exterior_assets/adaptive-icon.png",
          "backgroundColor": "#FFFFFF"
        },
        "package": "net.agency22.epc",
        "versionCode": 100060,
        "googleServicesFile": "./google-services.json",
        "permissions": []
      },
      "web": {
        "favicon": "./exterior_assets/favicon.png"
      }
  }
}